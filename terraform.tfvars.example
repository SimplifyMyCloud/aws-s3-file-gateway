# AWS S3 File Gateway Demo Configuration
# Copy this file to terraform.tfvars and customize as needed

# Demo name prefix (will be added to all resources)
demo_name = "smc-gateway-demo"

# S3 bucket name (must be globally unique)
s3_bucket_name = "smc-gateway-bucket-test-001"

# EC2 key pair name (must exist in your AWS account)
key_pair_name = "smc-dev-aws-keyz"

# CIDR block allowed to access NFS share (0.0.0.0/0 = everywhere - demo only!)
allowed_cidr = "0.0.0.0/0"